{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "tulip-browser",
  "version": "0.1.0",
  "identifier": "com.tulip-browser.app",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "tulip-browser",
        "width": 1700,
        "height": 900,
      }
    ],
    "security": {
      "csp": null,
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
